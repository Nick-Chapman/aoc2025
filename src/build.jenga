
executable-name:
  echo aoc.exe > $@

include ../../jenga/rulelib/haskell.jenga

main-entry:
  echo Advent2025 > $@

ghc-version:
  echo 9.10.3 > $@

ghc-options:
  echo -Werror >> $@
  echo -Wall >> $@
  echo -Wno-name-shadowing >> $@
  echo -O2 >> $@

# These dependencies are not discovered by gcc -M (not sure why)
xdepends:
  echo 'main.o : Timing.hi' >> $@

default-extensions:
  echo LambdaCase >> $@
  echo DuplicateRecordFields >> $@
  echo GADTs >> $@

packages:
  echo clock >> $@
